for i in range(img.shape[0]) : #parcours de toutes les lignes
    j=0 #compteur des colonnes
    pix=img[i,j,0]
    premiernoir,derniernoir=0,0
    while j<img.shape[1] and premiernoir==0 :
        j+=1
        if img[i,j,0]=0 :
            premiernoir=j
    while j<img.shape[1] :
        j+=1
        if img[i,j,0]=0 :
            derniernoir=j
    if derniernoir=!0 :
        break
    
